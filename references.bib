@inproceedings{xi-1997, 
year = {1997}, 
author = {Xi, Hongwei}, 
title = {{Weak and Strong Beta Normalisations in Typed Lambda-Calculi}}, 
booktitle = {Proceedings of Typed Lambda Calculi and Applications}, 
abstract = {{We present translation techniques to study relations between weak and strong beta-normalizations in various typed lambda-calculi. We first introduce a translation which translates lambda-terms into lambda I-terms, and show that every lambda-term is strongly beta-normalizable if and only if its translation is weakly beta-normalizable. We then prove that the translation preserves typability of lambda-terms in various lambda-calculi. This enables us to establish the equivalence between weak and strong beta-normalizations in these typed lambda-calculi. This translation can handle Curry typing as well as Church typing, strengthening some recent closely related results. This may bring some insights into answering whether weak and strong beta-normalization in all pure type systems are equivalent.}}, 
pages = {390--404}, 
volume = {1210}, 
shorttitle = {TLCA 1997}, 
series = {Lecture Notes in Computer Science}, 
publisher = {Springer}, 
keywords = {}
}

